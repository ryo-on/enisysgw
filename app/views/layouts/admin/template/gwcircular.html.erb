<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="ja" lang="ja">
<head>
<%= render :partial => 'layouts/admin/gw/header_tag' %>
<link rel="stylesheet" type="text/css" href="/layout/portal_1column/style.css" />
<link rel="stylesheet" type="text/css" href="/_common/themes/gw/css/style.css" />
</head>
<body <%= Page.body_id %>>
<div id="container">

<%= render :partial => 'layouts/admin/gw/piece_header' %>

<!--ここからバナー-->
<div id="banner">
<div id="bannerBody">
<%= render_component :controller => 'gw/admin/piece/header_menus', :action => 'index', :piece_param => '61' %>
</div>
</div>
<!--ここまでバナー-->
<!--ここからナビ2-->
<div id="navi2">
<div id="navi2Body">
</div>
</div>
<!--ここまでナビ-->
<!--ここからメインコンテンツ-->
<div id="main">
<div id="wrapper">
<!--ここからコンテンツ-->
<div id="content">
<div id="contentContainer">
<div id="contentHeader"></div>
<div id="contentBody">
<!--カラム用タグ-->

<!--ここから修正-->
<div class="bbsDocList circular">
<div id="FreezePane" class="FreezePaneOff">
   <div id="InnerFreezePane" class="InnerFreezePane"> </div>
</div>
<%= render :partial => 'gwcircular/admin/menus/piece_header' %>
<%= render :partial => 'gwcircular/admin/menus/piece_banner' %>
<div class="pieceBody">
<%= render_component :controller => 'gwcircular/admin/piece/menus', :action => 'index', :params => params %>

<div class="outer">
<div class="right">

<%= yield %>
</div><!--right-->
</div><!--outer-->

</div><!--pieceBody-->
</div><!--bbsDocList circular-->

<!--修正終わり-->
</div>
<div id="contentFooter"></div>
</div>
</div>
<!--ここまでコンテンツ-->
<!--ここから左サイド-->
<div id="menu">
<div id="menuContainer">
<div id="menuHeader"></div>
<div id="menuBody">
</div>
<div id="menuFooter"></div>
</div>
</div>
<!--ここまで左サイド-->
</div>
<!--ここから右サイド-->
<div id="links">
<div id="linksContainer">
<div id="linksHeader"></div>
<div id="linksBody">右サイド</div>
<div id="linksFooter"></div>
</div>
</div>
<!--ここまで右サイド-->
</div>
<!--ここまでメインコンテンツ-->
<%= render :partial => 'layouts/admin/gw/piece_footer' %>
</div>
</body>
</html>