<%= render :partial => 'header' unless request.path_info.include?("forward") %>
<%= form_for :item, :url => "#{gwcircular_menus_path}/#{@item.id}", :html => {:method => :put, :multipart => true, :id => 'itemForm'} do |f| %>

<%= f.error_messages %>
<div class="formTitle new">新規作成</div>
<%= render :partial => 'form', :locals => {:f => f} %>

<div class="submitters">
<div align="center">
<table>
<tr>
  <td><%= raw(submit_for_update(f, :js_submitter => "my_submit('public', 'new')", :caption => '配信', :id => 'item_submit')) %></td>
  <td><%= raw(submit_for_update(f, :js_submitter => "my_submit('draft', 'new')", :caption => '下書き', :id => 'item_submit_draft')) %></td>
  <% if request.path_info.include?("forward") %>
    <%= hidden_field_tag :request_path, request.path_info %>
    <td><%= raw(submit_for_update(f, :js_submitter => "my_submit('close', 'close')", :caption => '閉じる', :id => 'item_submit_close')) %></td>
  <% end %>
</tr>
</table>
</div>
</div>

<% end %>

