<div class="piece prop form">
  <div class="pieceHeader">
    <%= render :partial => 'piece_header' -%>
  </div>
  <div class="pieceBody">
    <div class="pieceBodyHeader">
      <%= render(:partial => '/gw/admin/prop_others/prop_master_menu') %>
      <%= render(:partial => '/gw/admin/prop_others/titleline') %>
    </div>
    <div class="pieceBodyBody">
      <span class="required">※は必須項目です。</span>
      <%= div_notice -%>
      <%= form_for :groupitem, :url => {:action => :update}, :html => {:method => :put, :multipart => true, :id => 'itemForm'} do |f| -%>
        <%= f.error_messages -%>
        <%= render :partial => 'form', :locals => {:f => f} -%>
        <%= raw(submit_for_update f) -%>
      <% end -%>
    </div>
  </div>
</div>
