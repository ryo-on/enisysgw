<div class="piece AdminMessage">
<div class="pieceHeader">
<h2 class="pieceTitle"><%= t("rumi.config_settings.scheduler.pref_executive.page_title") %></h2>
</div><!--pieceHeader-->
<div class="pieceBody">

<%= form_for :item, :url => "/gw/pref_executive_admins", :html => {:method => :create, :id => 'itemForm'} do |f| %>

<%= f.error_messages %>

<%= render :partial => 'form', :locals => {:f => f} %>

<%= submit_for_create f, :js_submitter => 'my_submit()', :caption => '保存' %>

<% end %>

</div><!--pieceBody-->
</div><!--piece-->