<%
  @edit_path = "/system/role_name_privs/#{@item.id}#{@params_set}"
%>
<div class="piece AdminMessage">

<%= render :partial => 'header' %>
<div class="pieceBody">
<%= form_for :item, :url => @edit_path, :html => {:method => :put, :multipart => true, :id => 'itemForm'} do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <%= submit_for(f, {:caption => '確定する'}) %>
<% end %>
</div>
</div>
