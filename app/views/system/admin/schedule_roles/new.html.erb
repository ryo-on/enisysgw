<div class="piece AdminMessage">
<%= render :partial => 'header' %>

<div class="pieceBody">
<%= form_for :item, :url => {:action => :create}, :html => {:id => 'itemForm'} do |f| %>
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <%= submit_for(f, {:caption => '登録する', :js_submitter => 'my_submit()',}) %>
<% end %>
</div>
</div>
